isbn = {1365-2656 (Electronic)$\backslash$n0021-8790 (Linking)},
issn = {00218790},
journal = {Journal of Animal Ecology},
keywords = {Capture-mark-recapture,Parasites,Radio-tracking,Social network analysis,Transmission},
number = {5},
pages = {1015--1022},
pmid = {19486206},
title = {{Comparison of social networks derived from ecological data: Implications for inferring infectious disease dynamics}},
volume = {78},
year = {2009}
