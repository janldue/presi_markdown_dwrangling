issn = {00029297},
journal = {American Journal of Human Genetics},
number = {1},
pages = {6--22},
pmid = {20074509},
publisher = {The American Society of Human Genetics},
title = {{Prioritizing GWAS Results: A Review of Statistical Methods and Recommendations for Their Application}},
url = {http://dx.doi.org/10.1016/j.ajhg.2009.11.017},
volume = {86},
year = {2010}
}
