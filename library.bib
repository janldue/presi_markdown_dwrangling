file = {:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/2013/Cozzoli et al/2013 - Cozzoli et al. - Application of non-linear quantile regression to macrozoobenthic species distribution modelling Comparing two co.pdf:pdf;:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/2013/Cozzoli et al/2013 - Cozzoli et al. - Application of non-linear quantile regression to macrozoobenthic species distribution modelling Comparing two(2).pdf:pdf},
isbn = {0171-8630},
issn = {01718630},
journal = {Marine Ecology Progress Series},
keywords = {Heteroskedasticity,Macrozoobenthos,Oosterschelde- westerschelde,Quantile regression,Regression splines,Sediment,Species distribution models},
