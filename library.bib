isbn = {0306-2619},
issn = {03062619},
journal = {Applied Energy},
keywords = {Bayesian network,Fault diagnosis,Ground-source heat pump,Multi-source information fusion},
pages = {1--9},
publisher = {Elsevier Ltd},
title = {{Multi-source information fusion based fault diagnosis of ground-source heat pump using Bayesian network}},
url = {http://dx.doi.org/10.1016/j.apenergy.2013.09.043},
volume = {114},
year = {2014}
