@article{Cellot1994,
abstract = {1. This paper develops a framework of spatial and temporal variability$\backslash$nfor a habitat typology of the Upper Rhone River (France) and its$\backslash$nalluvial floodplain that is based on about 17 years of data collection$\backslash$nand analysis. The aim was to provide a scale of spatial-temporal$\backslash$nvariability for river habitat templet predictions on trends in species$\backslash$ntraits and species richness.$\backslash$n2. In developing this framework, eight physical-chemical variables were$\backslash$navailable and could be considered for twenty-two habitat types:$\backslash$nseventeen superficial (surface) and five interstitial (0.5 m below the$\backslash$nsubstrate surface). These habitat types were selected in two areas (Jons$\backslash$nand Bregnier-Cordon) after geomorphological considerations.and because$\backslash$nof differences in their biological characteristics.$\backslash$n3. The data sets used were processed by a `fuzzy coding' method using,$\backslash$nfor each variable, the frequency distribution (by modalities =$\backslash$ncategories) of all measurements and monthly means over an annual scale.$\backslash$nTwo tables were produced; the first corresponded to an expression of the$\backslash$ntotal variability, and the second represented an evaluation of the$\backslash$ntemporal variability.$\backslash$n4. Each of these tables was analysed by correspondence analysis, which$\backslash$nprovided factorial scores that were used to calculate, by habitat type$\backslash$nand by variable, a total variability and a temporal variability in terms$\backslash$nof cumulated variability of factorial scores for the eight$\backslash$nphysical-chemical variables. The rationale in describing variability$\backslash$nfrom these two tables is that total variability equals temporal$\backslash$nvariability plus spatial variability. The spatial variability was then$\backslash$ndetermined by the difference between total and temporal variability.$\backslash$nFrom this procedure, a positioning of the twenty-two habitat types on$\backslash$nthe spatial and temporal variability axes was obtained.$\backslash$n5. The estimate of spatial variability did not consider any error term$\backslash$nthat may have occurred in the above model; it was then tested by an$\backslash$nindependent assessment of the spatial variability using thirteen$\backslash$nvariables in nine major habitat types. A high correlation between the$\backslash$ntwo ways of assessing spatial variability (r = 0.85, P {\textless} 0.004)$\backslash$nunderscored the reliability of the spatial variability that was$\backslash$ncalculated previously.$\backslash$n6. The river habitat templet obtained for the Upper Rhone and its$\backslash$nalluvial floodplain appears to be appropriate to test the predictions on$\backslash$npatterns of species traits and species richness in the framework of$\backslash$nspatial and temporal variability.},
author = {Cellot, Bernard and Dole-Olivier, Marie Jose and Bornette, Gudrun and Pautou, Guy},
doi = {10.1111/j.1365-2427.1994.tb01743.x},
file = {:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/1994/Cellot et al/1994 - Cellot et al. - Temporal and spatial environmental variability in the Upper Rhone River and its floodplain.pdf:pdf},
isbn = {0046-5070},
issn = {0046-5070},
journal = {Freshwater Biology},
number = {3},
pages = {311--325},
pmid = {2294},
title = {{Temporal and spatial environmental variability in the Upper Rhone River and its floodplain}},
url = {http://doi.wiley.com/10.1111/j.1365-2427.1994.tb01743.x},
volume = {31},
year = {1994}
}
@article{Lenat1988,
abstract = {A standardized qualitative sampling technique was developed and tested for shallow streams in N. Carolina.  This method emphasizes multihabitat collections and the use of both coarse- and fine-mesh samplers.  All samples are picked in the field, a process facilitated by field-preservation of some samples.  The qualitative method was found to be more rapid than a semiquantitative technique (kick nets), yet produced a grater number of taxa per site.  Different taxa richness criteria were developed for each ecoregion to assign water quality classification, and these values were shown to be related to more conventional physical/chemical measurements  This method has potential as a rapid, inexpensive biomonitoring tool.},
