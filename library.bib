isbn = {0950-7051},
issn = {09507051},
journal = {Knowledge-Based Systems},
keywords = {Computation sharing,Diagnostic Bayesian networks,Exact inference,Inference simplifying,Search},
pages = {95--103},
publisher = {Elsevier B.V.},
title = {{A search problem in complex diagnostic Bayesian networks}},
