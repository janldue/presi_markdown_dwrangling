isbn = {0950-2688 {\%}[ April 20, 2007},
issn = {0950-2688},
journal = {Epidemiology and infection},
keywords = {Animals,Cattle,Crohn Disease,Crohn Disease: epidemiology,Crohn Disease: microbiology,Humans,Mycobacterium avium subsp. paratuberculosis,Paratuberculosis,Paratuberculosis: complications,Paratuberculosis: epidemiology,Paratuberculosis: microbiology},
number = {7},
