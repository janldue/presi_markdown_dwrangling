issn = {1873-3336},
journal = {Journal of hazardous materials},
keywords = {Animals,No-Observed-Adverse-Effect Level,Risk Assessment,Rivers,Rivers: chemistry,Spain,Stress, Physiological,Water Pollutants, Chemical,Water Pollutants, Chemical: analysis,Water Pollutants, Chemical: standards,Water Pollutants, Chemical: toxicity},
