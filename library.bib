publisher = {Elsevier Ltd},
title = {{Automated classification of brain tumours from short echo time in vivo MRS data using Gaussian Decomposition and Bayesian Neural Networks}},
url = {http://dx.doi.org/10.1016/j.eswa.2014.02.031},
volume = {41},
year = {2014}
