@article{Usepa2000,
abstract = {Since the inception of the Clean Water Act (CWA) in 1972, the rivers,$\backslash$nlakes, estuaries, and wetlands of the United States have indeed become$\backslash$ncleaner. The standard for measuring these improvements are both chemical$\backslash$nand biological. Yet, we know that many waterbodies still fail to$\backslash$nmeet the goal of the Clean Water Act - to maintain the chemical,$\backslash$nphysical and biological integrity of the nation's waters. Biological$\backslash$nassessments have become increasingly important tools for managing$\backslash$nwater quality to meet the goals of the CWA. These methods, which$\backslash$nuse measurements of aquatic biological communities, are particularly$\backslash$nimportant for evaluating the impacts of chemicals for which there$\backslash$nare no water quality standards, and for non-chemical stressors such$\backslash$nas flow alteration, siltation, and invasive species. However, although$\backslash$nbiological assessments are critical tools for detecting impairment,$\backslash$nthey do not identify the cause or causes of the impairment. The Office$\backslash$nof Water and Office of Research and Development of the US EPA have$\backslash$ndeveloped a process for identifying any type of stressor or combination$\backslash$nof stressors that cause biological impairment. The Stressor Identification$\backslash$n(SI) Guidance is intended to lead water resource managers through$\backslash$na formal and rigorous process that identifies stressors causing biological$\backslash$nimpairment in aquatic ecosystems, and provides a structure for organizing$\backslash$nthe scientific evidence supporting the conclusions. The ability to$\backslash$naccurately identify stressors and defend the evidence supporting$\backslash$nthose findings is a critical step in developing strategies that will$\backslash$nimprove the quality of aquatic resources. The Stressor Identification$\backslash$nprocess (SI) is prompted by biological assessment data indicating$\backslash$nthat a biological impairment has occurred. The general SI process$\backslash$nentails critically reviewing available information, forming possible$\backslash$nstressor scenarios that might explain the impairment, analyzing those$\backslash$nscenarios, and producing conclusions about which stressor or stressors$\backslash$nare causing the impairment. The SI process is iterative, usually$\backslash$nbeginning with a retrospective analysis of available data. The accuracy$\backslash$nof the identification depends on the quality of data and other information$\backslash$nused in the SI process. In some cases, additional data collection$\backslash$nmay be necessary to accurately identify the stressor(s). The conclusions$\backslash$ncan be translated into management actions and the effectiveness of$\backslash$nthose management actions can be monitored.},
author = {Usepa and Epa},
file = {:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/2000/Usepa, Epa/2000 - Usepa, Epa - Stressor identification guidance document.pdf:pdf},
isbn = {EPA-822-B-00-025},
journal = {EPA-822-B00-025. United States Environmental Protection Agency, Washington, DC.},
pages = {228},
title = {{Stressor identification guidance document}},
year = {2000}
}
