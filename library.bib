}
@article{Colas2014,
abstract = {We propose that a niche-based experimental approach at population level could be used to solve some uncertainties in traditional approaches in ecotoxicology. We tested this approach in the context of multiple stressors (i.e. chemical and physical) in a selection of six run-of-river reservoirs with different levels of sediment contamination and associated upstream and downstream river sites. A niche-based approach was tested using three functional traits (habitat, food preferences and body size) and discrepancy between the realized and theoretical niches. We first identified three groups of taxa and then recorded differences along the disturbance gradients, such as an increase in competition, a narrowing of spatial and trophic niche breadth (e.g. of Leuctra major and Gammarus pulex), a widening of spatial niche breadth (e.g. of Ephemerella ignita), a greater proportion of small individuals (e.g. of G. pulex) and a decreasing or an increasing (e.g. L. major) discrepancy between realized and theoretical niches.},
author = {Colas, Fanny and Vigneron, Amandine and Felten, Vincent and Devin, Simon},
doi = {10.1016/j.envpol.2013.09.033},
file = {:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/2014/Colas et al/2014 - Colas et al. - The contribution of a niche-based approach to ecological risk assessment using macroinvertebrate species under mul.pdf:pdf},
issn = {1873-6424},
journal = {Environmental pollution (Barking, Essex : 1987)},
keywords = {Amphipoda,Amphipoda: physiology,Animals,Ecology,Ecosystem,Environmental Monitoring,Environmental Monitoring: methods,Physiological,Risk Assessment,Rivers,Stress,Water Pollution,Water Pollution: analysis,ecological risk assessment,macroinvertebrates,multiple stressors},
mendeley-tags = {ecological risk assessment,macroinvertebrates,multiple stressors},
month = {feb},
pages = {24--34},
pmid = {24212068},
publisher = {Elsevier Ltd},
title = {{The contribution of a niche-based approach to ecological risk assessment: using macroinvertebrate species under multiple stressors.}},
