journal = {The Science of the total environment},
keywords = {Animals,Biological,Conservation of Natural Resources,Conservation of Natural Resources: methods,Ecosystem,France,Invertebrates,Invertebrates: physiology,Models,Population Dynamics,Risk Assessment,Rivers,Trees,WFD,Water Quality,Water Quality: standards,benthic macroinvertebrates,biological traits,risk assessment},
mendeley-tags = {WFD,benthic macroinvertebrates,biological traits,risk assessment},
