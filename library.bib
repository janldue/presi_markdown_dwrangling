journal = {International journal of epidemiology},
keywords = {bias,causal diagrams,causality,confounding,data analysis,direct effects,epidemiological methods,graphical models,inference,instrumental variables,risk analysis,structural equations,sufficient-component cause models},
number = {5},
pages = {1030--1037},
