@article{Vanderweele2008,
abstract = {Sufficient-component causes are discussed within the deterministic potential outcomes framework so as to formalize notions of sufficient causes, synergism and sufficient cause interactions. Doing so allows for the derivation of counterfactual and empirical conditions for detecting the presence of sufficient cause interactions. The conditions are novel in that, unlike other conditions in the literature, they make no assumption about monotonicity. The conditions can also be generalized and the conditions for three-way sufficient cause interactions are given explicitly. The statistical tests derived for sufficient cause interactions are compared with and contrasted to interaction terms in standard statistical models.},
author = {Vanderweele, Tyler J. and Robins, James M.},
doi = {10.1093/biomet/asm090},
file = {:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/2008/Vanderweele, Robins/2008 - Vanderweele, Robins - Empirical and counterfactual conditions for sufficient cause interactions.pdf:pdf},
