keywords = {Animals,Biodiversity,Chemical,Chemical: analysis,Chemical: toxicity,Ecosystem,Environmental Monitoring,Invertebrates,Invertebrates: classification,Invertebrates: drug effects,Invertebrates: growth {\&} development,Pesticides,Pesticides: analysis,Pesticides: toxicity,Rivers,Rivers: chemistry,Water Pollutants,macroinvertebrates,multiple stressors,pesticide stress,risk assessment},
