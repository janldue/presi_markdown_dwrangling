issn = {1879-1514},
journal = {Aquatic toxicology (Amsterdam, Netherlands)},
keywords = {Analysis of Variance,Animals,Chromatography, High Pressure Liquid,Daphnia,Daphnia: drug effects,Daphnia: growth {\&} development,Diptera,Diptera: physiology,Fungicides, Industrial,Fungicides, Industrial: toxicity,Germany,Larva,Larva: physiology,Population Dynamics,Predatory Behavior,Predatory Behavior: physiology,Pyrimidines,Reproduction,Reproduction: physiology,Stress, Physiological,Stress, Physiological: physiology,Temperature},
month = {aug},
