issn = {1879-1026},
journal = {The Science of the total environment},
keywords = {Agriculture,Ecosystem,Pesticides,Pesticides: adverse effects,Pesticides: analysis,Plant Leaves,Plant Leaves: metabolism,Rivers,Rivers: chemistry,Water Pollutants, Chemical,Water Pollutants, Chemical: adverse effects,Water Pollutants, Chemical: analysis},
month = {feb},
pages = {148--55},
pmid = {22177030},
