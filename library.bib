file = {:C$\backslash$:/Users/admin/sciebo/AOE{\_}Promotion/Literatur{\_}sortiert/2006/Christensen et al/2006 - Christensen et al. - Multiple anthropogenic stressors cause ecological surprises in boreal lakes.pdf:pdf},
